<%= t('rent_title', :locale => @user.locale) %>
<% @book.attributes.except("updated_at", "created_at", "id").each do |attribute| %>
  <%= t(attribute[0], :locale => @user.locale) %> <%= attribute[1] %>
<% end %>
<%= t('created_at', :locale => @user.locale) %> <%= @rent.created_at %>
<%= t('rent_start_date', :locale => @user.locale) %> <%= @rent.start_date %>
<%= t('rent_end_date', :locale => @user.locale) %> <%= @rent.end_date %>
